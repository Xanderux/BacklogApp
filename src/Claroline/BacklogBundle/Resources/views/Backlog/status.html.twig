{% extends '::base.html.twig' %}

{% block body %}
<h1> Status </h1>

<table>
    <thead>
        <th>
            <tr>
                <td>Status</td>
            </tr>
        </th>
    </thead>
    <tbody>
        {% for statu in status %}
            <tr>
                <td>{{ statu.getStatus() }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('new_status_form') }}">Create</a>

{%endblock%}
