{% extends '::base.html.twig' %}

{% block body %}
<h1> Versions </h1>

<table class="table table-striped">
    <thead>
            <tr>
                <th>Version</th>
            </tr>
    </thead>
    <tbody>
        {% for version in versions %}
            <tr>
                <td>{{ version.getVersionName() }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('create_version') }}">Create</a>

{%endblock%}
