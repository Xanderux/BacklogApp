{% extends '::base.html.twig' %}

{% block body %}
<h1> Roles </h1>

<table class="table table-striped">
    <thead>
            <tr>
                <th>Role</th>
            </tr>
    </thead>
    <tbody>
        {% for role in roles %}
            <tr>
                <td>{{ role.getName() }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('create_role') }}">Create</a>

{%endblock%}
