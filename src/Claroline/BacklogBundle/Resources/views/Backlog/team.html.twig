{% extends '::base.html.twig' %}

{% block body %}
<h1> Teams </h1>

<table class="table table-striped">
    <thead>
            <tr>
                <th>Team</th>
            </tr>
    </thead>
    <tbody>
        {% for team in teams %}
            <tr>
                <td>{{ team.getName() }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

<a href="{{ path('create_team') }}">Create</a>

{%endblock%}
